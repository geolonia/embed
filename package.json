    "@geolonia/mbgl-geolonia-control": "^0.3.8",
    "@mapbox/geojson-extent": "^0.3.2",
    "@tilecloud/mbgl-gesture-handling": "git+https://github.com/geolonia/mbgl-gesture-handling.git",
    "@turf/center": "^6.0.1",
    "core-js": "^2.6.8",
    "intersection-observer": "^0.5.1",
    "promise-polyfill": "^8.1.0",
