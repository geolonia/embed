    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "webpack": "^4.20.2",
