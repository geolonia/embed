{
  "name": "@tilecloud/js",
  "version": "0.2.4",
  "description": "tilecloud API",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config ./sample/webpack.config.js",
    "build": "babel ./src --out-dir lib --ignore ./src/tilecloud.js",
    "build:tilecloud": "webpack -p  --config ./webpack.config.js",
    "format": "prettier-eslint \"./src/**/*.js\" --write",
    "lint": "eslint \"src/**/*.js\"",
    "test": "mocha",
    "deploy": "npm run build:tilecloud && cp ./dist/tilecloud.js ./sample/tilecloud.js && gh-pages --dist ./sample"
  },
  "author": "kamataryo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tilecloud/tilecloud.js.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tilecloud/tilecloud.js/issues"
  },
  "homepage": "https://github.com/tilecloud/tilecloud.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@tilecloud/mbgl-tilecloud-control": "^0.3.5",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.1.0",
    "eslint": "^5.3.0",
    "gh-pages": "^2.0.1",
    "jsdom": "^13.2.0",
    "mapbox-gl": "^0.52.0-beta.2",
    "mocha": "^5.2.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "style-loader": "^0.23.1",
    "url-parse": "^1.4.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.14"
  },
  "peerDependencies": {
    "@babel/polyfill": "^7",
    "@tilecloud/mbgl-tilecloud-control": "^0",
    "mapbox-gl": "^0.52"
  }
}
